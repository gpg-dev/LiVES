#: src/main.c:2609
#, c-format
msgid ""
"\n"
"Startup syntax is: %s [opts] [filename [start_time] [frames]]\n"
msgstr ""
"\n"
"A sintaxe de inicialização é: %s [opçoes] [nome_do_arquivo [tempo_inicial] "
"[quadros]]\n"

#: src/main.c:2610
msgid "Where: filename is the name of a media file or backup file.\n"
msgstr "Onde: nome_do_arquivo é o nome de um arquivo de mídia ou de backup.\n"

#: src/main.c:2611
msgid "start_time : filename start time in seconds\n"
msgstr "tempo_inicial : tempo inicial do arquivo em segundos\n"

#: src/main.c:2612
msgid "frames : maximum number of frames to open\n"
msgstr "quadros : número máximo de quadros a abrir\n"

#: src/main.c:2614
msgid "opts can be:\n"
msgstr "as opções podem ser:\n"

#: src/main.c:2615
msgid "-help            : show this help text and exit\n"
msgstr "-help            : mostrar este texto de ajuda e sair\n"

#: src/main.c:2616
msgid "-tmpdir <tempdir>: use alternate working directory (e.g /var/ramdisk)\n"
msgstr ""
"-tmpdir <tempdir>: use algum diretório alternativo (ex.: /var/ramdisk)\n"

#: src/main.c:2617
msgid "-set <setname>   : autoload clip set setname\n"
msgstr "-set <nome_do_set>   : autocarregar conjunto de clips nome_do_set\n"

#: src/main.c:2618
msgid "-noset           : do not load any set on startup\n"
msgstr "-noset           : não carregar conjunto de clipes no início\n"

#: src/main.c:2619
msgid "-norecover       : force no-loading of crash recovery\n"
msgstr "-norecover       : forçar não carregamento da recuperação de crash\n"

#: src/main.c:2620
msgid "-recover         : force loading of crash recovery\n"
msgstr "-recover         : forçar carregamento da recuperação de crash\n"

#: src/main.c:2621
msgid ""
"-nothreaddialog  : does nothing - retained for backwards compatibility\n"
msgstr "-nothreaddiaog: não faz nada - retida para compatibilidade\n"

#: src/main.c:2622
msgid "-nogui           : do not show the gui\n"
msgstr "-nogui           : não mostrar a interface gráfica\n"

#: src/main.c:2623
msgid "-nosplash        : do not show the splash window\n"
msgstr "-nosplash: não mostrar a janela de splash\n"

#: src/main.c:2624
msgid "-noplaywin       : do not show the play window\n"
msgstr "-noplaywin : não mostrar a janela de vídeo\n"

#: src/main.c:2625
msgid "-noninteractive  : disable menu interactivity\n"
msgstr "-noninteractive: desabilitar interatividade com menus\n"

#: src/main.c:2626
msgid "-startup-ce      : start in clip editor mode\n"
msgstr "-startup-ce      : iniciar no modo de edição de clipe\n"

#: src/main.c:2627
msgid "-startup-mt      : start in multitrack mode\n"
msgstr "-startup-mt      : iniciar no modo de multi-pista\n"

#: src/main.c:2628
msgid ""
"-fxmodesmax <n>  : allow <n> modes per effect key (minimum is 1, default is "
"8)\n"
msgstr ""
"-fxmodesmax <n> : permite <n> modos por tecla de efeito (mínimo 1, padrão "
"8)\n"

#: src/main.c:2630
msgid "-oscstart <port> : start OSC listener on UDP port <port>\n"
msgstr "-oscstart <port> : iniciar monitoramento de OSC na porta UDP <port>\n"

#: src/main.c:2631
msgid "-nooscstart      : do not start OSC listener\n"
msgstr "-nooscstart      : não iniciar monitoramento de OSC\n"

#: src/main.c:2634
#, c-format
msgid ""
"-asource <source>          : set the initial audio source; <source> can be "
"'internal' or 'external' (only valid for %s and %s players)\n"
msgstr ""
"-asource <fonte> : define a fonte de áudio inicial; <fonte> pode ser "
"'internal' ou 'external' (válido apenas para tocadores %s e %s )\n"

#: src/main.c:2636
msgid "-aplayer <ap>    : start with selected audio player. <ap> can be "
msgstr ""
"-aplayer <ap> : inicia com o reprodutor de áudio selecionado. <ap> pode ser "

#: src/main.c:2648
msgid ""
"-jackopts <opts>    : opts is a bitmap of jack startup options \n"
"                      [1 = jack transport client, \n"
"                      2 = jack transport master, \n"
"                      4 = start/stop jack transport server, \n"
"                      8 = pause transport when video paused, \n"
"                      16 = start/stop jack audio server] \n"
msgstr ""
"-jackopts <opts> : opts é um bitmap da opções de inicialização do jack \n"
"                     [1 = cliente de transporte de jack, \n"
"                     2 = mestre de transporte jack, \n"
"                     4 = iniciar/parar o servidor de transporte jack, \n"
"                     8 = pausar o transporte quando o vídeo é pausado, \n"
"                     16 = iniciar/parar o servidor de áudio do jack] \n"

#: src/main.c:2667
msgid " or "
msgstr " ou "

#: src/main.c:2674
msgid "-devicemap <mapname>          : autoload devicemap\n"
msgstr "-devicemap <mapname>          : autocarregar devicemap\n"

#: src/main.c:2676
msgid ""
"-vppdefaults <file>          : load video playback plugin defaults from "
"<file> (Note: only sets the settings, not the plugin type)\n"
msgstr ""
"-vppdefaults <arquivo> : carregar os padrões de plugin para tocar vídeos do "
"<arquivo> (Nota: apenas define as configurações, não o tipo de plugin)\n"

#: src/main.c:2677
msgid "-debug            : try to debug crashes (requires 'gdb' installed)\n"
msgstr ""
"-debug            : tentar depurar falhas (necessita ter 'gdb' instalado)\n"

